<div class="container mx-auto px-4 py-6">
  
  <% if Current.user.manager? %>
    <%= render 'manager', questionnaires: @questionnaires, students: @students, questions: @questions, user_questionnaires: @user_questionnaires %>
  <% end %>

  <% if Current.user.student? %>
    <%= render 'student', user_questionnaires: @user_questionnaires, completed_questionnaires: @completed_questionnaires, incompleted_questionnaires: @incompleted_questionnaires %>
  <% end %>
</div>
