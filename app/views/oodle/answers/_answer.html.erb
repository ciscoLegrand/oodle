<div id="<%= dom_id answer %>">
  <p>
    <strong>User:</strong>
    <%= answer.user_id %>
  </p>

  <p>
    <strong>Answered at:</strong>
    <%= answer.answered_at %>
  </p>

  <p>
    <strong>Result:</strong>
    <%= answer.result %>
  </p>

  <p>
    <strong>Score:</strong>
    <%= answer.score %>
  </p>

  <p>
    <strong>Questionnaire:</strong>
    <%= answer.questionnaire_id %>
  </p>

  <p>
    <strong>Question:</strong>
    <%= answer.question_id %>
  </p>

</div>
